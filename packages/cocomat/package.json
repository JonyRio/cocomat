{
  "name": "coco",
  "version": "0.0.1",
  "description": "cocomat",
  "author": "tencent",
  "main": "main.js",
  "runtime-resource": {
    "path": "../../coco-mat",
    "name": "mat"
  },
  "reload": {
    "ignore": ["../../coco-mat/**/*", "settings/**/*"]
  },
  "main-menu": {
    "i18n:MAIN_MENU.package.title/cocomat/安装": {
      "message": "coco:install"
